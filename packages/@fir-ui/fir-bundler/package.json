{
  "name": "@fir-ui/bundler",
  "version": "0.3.0",
  "description": "Library bundler for fir-ui",
  "main": "src/bundle.js",
  "author": "fjc0k",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": "src/cli.js",
  "files": [
    "src"
  ],
  "firb": {
    "entry": "test/**/*.js",
    "format": "es,cjs,umd,cjs-min,umd-min,es-min",
    "alias": {
      "@": "test"
    }
  },
  "scripts": {
    "test": "node src/cli.js"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@fir-ui/babel-preset-library": "^0.0.9",
    "cac": "^4.4.4",
    "first-commit-date": "^0.2.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.5",
    "path-depth": "^0.2.0",
    "postcss-modules-resolve-imports": "^1.3.0",
    "rollup": "^0.57.1",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^4.0.0-beta.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.4.1",
    "rollup-plugin-uglify": "^3.0.0",
    "stringify-author": "^0.1.3",
    "use-config": "^2.0.4"
  }
}
