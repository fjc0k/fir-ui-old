{
  "name": "@fir-ui/bundler",
  "version": "0.5.0",
  "description": "Library bundler for fir-ui",
  "main": "src/bundle.js",
  "author": "fjc0k",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "bundler": "./src/cli.js"
  },
  "files": [
    "src"
  ],
  "firb": {
    "entry": "test/index.js",
    "format": "cjs",
    "alias": {
      "@": "test"
    },
    "dest": "test/dist",
    "postcss": {
      "modules": true
    }
  },
  "scripts": {
    "test": "node src/cli.js"
  },
  "browserslist": [
    "iOS 6",
    "Android 4.0"
  ],
  "dependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@fir-ui/babel-preset-library": "^0.0.11",
    "autoprefixer": "^8.2.0",
    "cac": "^4.4.4",
    "first-commit-date": "^0.2.0",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.5",
    "path-depth": "^0.2.0",
    "rollup": "^0.57.1",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^4.0.0-beta.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.4.1",
    "rollup-plugin-uglify": "^3.0.0",
    "stringify-author": "^0.1.3",
    "use-config": "^2.0.4"
  }
}
