$font = {
  size: {
    root: 16px,
    smaller: .4rem,
    small: .6rem,
    base: 1rem,
    medium: 1rem,
    large: 1rem,
    larger: 1.2rem
  },
  family: '-apple-system-font, Helvetica Neue, Helvetica, sans-serif'
}

$icon = {
  font: {
    path: '../assets/iconfont.ttf',
    family: 'f-icon',
    size: 1em
  }
}

@import "../settings";

$color.background.main = #fff